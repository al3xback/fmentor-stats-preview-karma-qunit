{
	"name": "fmentor-stats-preview-karma-qunit",
	"private": true,
	"version": "1.0.0",
	"author": "alex <al3xback@gmail.com>",
	"license": "ISC",
	"scripts": {
		"sass-dev": "sass --watch scss/main.scss css/main.css",
		"sass-prod": "sass scss/main.scss css/main.css --style=compressed --no-source-map",
		"postcss": "postcss css/main.css -o css/main.css --use autoprefixer --no-map",
		"prettier": "prettier --write .",
		"dev": "run-s sass-dev",
		"lint": "run-s prettier",
		"test": "karma start",
		"build": "run-s lint sass-prod postcss"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.16",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-qunit": "^4.1.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.31",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.8",
		"qunit": "^2.20.0",
		"sass": "^1.62.1"
	},
	"browserslist": [
		"defaults"
	]
}
